{
  "name": "attendflow-server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "npm run schema:merge && prisma generate && tsc",
    "postinstall": "npm run schema:merge && prisma generate",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "ts-node-dev --respawn --transpile-only ./src/server.ts",
    "lint": "npx eslint src --ignore-pattern .ts",
    "lint:fix": "npx eslint src --fix",
    "prettier": "prettier --ignore-path .gitignore --write \"./src/**/*.+(js|ts|json)\"",
    "prettier:fix": "npx prettier --write src",
    "resource": "node .bin/cli.js resource",
    "schema:merge": "node scripts/merge-schemas.js",
    "prisma:generate": "npm run schema:merge && prisma generate",
    "prisma:migrate": "npm run schema:merge && prisma migrate dev",
    "prisma:studio": "npm run schema:merge && prisma studio",
    "prisma:seed": "ts-node prisma/seed.ts",
    "prisma:format": "npm run schema:merge && prisma format",
    "db:push": "npm run schema:merge && prisma db push",
    "db:pull": "prisma db pull",
    "seed:admin": "ts-node src/scripts/seed-admin.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/adapter-pg": "^7.1.0",
    "@prisma/client": "^7.2.0",
    "@types/cors": "^2.8.19",
    "@types/multer": "^2.0.0",
    "bcrypt": "^6.0.0",
    "cookie": "^1.0.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.6",
    "pg": "^8.16.3",
    "prisma": "^7.2.0",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@prisma/config": "^7.2.0",
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/nodemailer": "^7.0.1",
    "@types/pg": "^8.16.0",
    "commander": "^14.0.1",
    "eslint": "^9.35.0",
    "globals": "^16.4.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.43.0"
  }
}
